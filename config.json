{
  "allowed_extensions": [
    ".py",
    ".java",
    ".cpp",
    ".c",
    ".h",
    ".js",
    ".ts",
    ".jsx",
    ".tsx",
    ".html",
    ".css",
    ".scss",
    ".sass",
    ".sql",
    ".md",
    ".yaml",
    ".yml",
    ".xml",
    ".json",
    ".cs",
    ".go",
    ".rs",
    ".swift",
    ".kt",
    ".php",
    ".rb",
    ".sh",
    ".bash",
    ".m",
    ".mm",
    ".shader",
    ".compute",
    ".cginc",
    ".txt",
    ".vue",
    ".svelte"
  ],
  "ignore_dirs": [
    ".git",
    ".idea",
    ".vscode",
    "__pycache__",
    "node_modules",
    "venv",
    "env",
    ".env",
    "build",
    "dist",
    "bin",
    "obj",
    "out",
    "target",
    "cmake-build-debug",
    "cmake-build-release",
    ".gradle",
    "gradle",
    "Library",
    "Temp",
    "Logs",
    "UserSettings",
    "Packages",
    "Drivers",
    "Middlewares",
    "CMSIS",
    "MDK-ARM",
    "EWARM",
    "cmake",
    "DebugVals",
    "Docs",
    "Doc",
    ".next",
    ".nuxt",
    "coverage",
    ".pytest_cache",
    ".mypy_cache",
    ".tox"
  ],
  "ignore_prefixes": [
    "stm32f4xx_it",
    "system_stm32f4xx",
    "stm32f4xx_hal_conf",
    "stm32f4xx_hal_msp",
    "sysmem",
    "syscalls",
    "stm32f4xx_hal_timebase_tim"
  ],
  "ignore_files": [
    ".DS_Store",
    "Thumbs.db",
    "desktop.ini",
    "*.pyc",
    "*.pyo",
    "*.pyd",
    ".Python",
    "package-lock.json",
    "yarn.lock",
    "pnpm-lock.yaml"
